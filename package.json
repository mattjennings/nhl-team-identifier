{
  "name": "team-finder",
  "version": "2.1.0",
  "license": "ISC",
  "homepage": "https://github.com/mattjennings/team-finder",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattjennings/team-finder"
  },
  "description": "Finds NHL teams based on given text",
  "main": "lib/main/index.js",
  "typings": "lib/main/index.d.ts",
  "module": "lib/module/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:main && npm run build:module && npm run babel",
    "build:main": "tsc -p tsconfig.build.json",
    "build:module": "tsc -p tsconfig.module.json",
    "babel": "babel lib --out-dir lib --extensions \".js\"",
    "clean": "rimraf lib",
    "test": "jest"
  },
  "keywords": [
    "nhl",
    "team",
    "finder",
    "identifier"
  ],
  "author": "Matt Jennings <mattjennings44@gmail.com> (http://mattjennings.me)",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  }
}
